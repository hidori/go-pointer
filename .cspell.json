{
  "version": "0.2",
  "language": "en",
  "ignorePaths": [".cspell.json", ".golangci.yml"],
  "words": [
    "hidori",
    "hiroaki",
    "interactor",
    "Println",
    "shibuki",
    "struct",
    "usecase"
  ],
  "languageSettings": [
    {
      "languageId": "go",
      "allowCompoundWords": false,
      "includeRegExpList": ["CStyleComment", "string"],
      "ignoreRegExpList": [
        "import\\s*\\((.|[\r\n])*?\\)",
        "import\\s*.*\".*?\"",
        "//\\s*go:generate.*",
        "//\\s*nolint:.*",
        "//\\s*cSpell:.*"
      ]
    }
  ]
}
